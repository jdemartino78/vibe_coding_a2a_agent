# This is the central configuration file for the entire project.
# Run `cp .env.example .env` and fill in the values below.

# --- Google Cloud Project Configuration ---
GOOGLE_CLOUD_PROJECT="your-project-id"
PROJECT_NUMBER="your-project-number"
GOOGLE_CLOUD_LOCATION="us-central1"
BUCKET_NAME="your-unique-bucket-name"

# --- API Keys & Model Backend ---
# Choose Model Backend: True for Vertex AI, False for ML Dev
GOOGLE_GENAI_USE_VERTEXAI=True
# GOOGLE_API_KEY="your-google-api-key" # Only needed if GOOGLE_GENAI_USE_VERTEXAI is False

# --- MCP Server URLs ---
# These will be populated after you deploy the MCP servers.
CT_MCP_SERVER_URL=""
WEA_MCP_SERVER_URL=""

# --- Agent Engine IDs & URLs ---
# These will be populated after you deploy the agents.
COCKTAIL_AGENT_ENGINE_ID=""
COCKTAIL_AGENT_URL=""
WEATHER_AGENT_ENGINE_ID=""
WEA_AGENT_URL=""
HOSTING_AGENT_ENGINE_ID=""
HOSTING_AGENT_URL=""

# --- Service Names ---
# You can generally leave these as default.
COCKTAIL_REMOTE_MCP_SERVER_NAME='cocktail-remote-mcp-server'
WEATHER_REMOTE_MCP_SERVER_NAME='weather-remote-mcp-server'
FRONTEND_SERVICE_NAME='adk-frontend'
